{
  "name": "jobcamp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node build/index.js",
    "dev": "vite dev --port 34040",
    "build": "vite build",
    "build:no-check": "SKIP_TYPE_CHECK=true vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --no-tsconfig --ignore '**/ui/**'",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint .",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "prepare": "husky"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.js"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^3.3.1",
    "@sveltejs/adapter-node": "^5.2.13",
    "@sveltejs/kit": "^2.42.2",
    "@sveltejs/vite-plugin-svelte": "^4.0.4",
    "@tailwindcss/typography": "^0.5.16",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/svelte": "^5.2.8",
    "@types/eslint": "^9.6.1",
    "@types/node": "^22.17.0",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "bits-ui": "1.0.0-next.84",
    "clsx": "^2.1.1",
    "eslint": "^9.32.0",
    "eslint-plugin-svelte": "^2.46.1",
    "globals": "^15.15.0",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.1.2",
    "lucia": "^3.2.2",
    "lucide-svelte": "^0.460.1",
    "postcss": "^8.5.6",
    "prisma": "^6.13.0",
    "svelte": "^5.37.1",
    "svelte-check": "^4.3.0",
    "tailwind-merge": "^2.6.0",
    "tailwind-variants": "^0.3.1",
    "tailwindcss": "^3.4.17",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.38.0",
    "vite": "^5.4.20",
    "vitest": "^3.2.4"
  },
  "type": "module",
  "lint-staged": {
    "*.{js,ts,svelte}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@google-cloud/storage": "^7.16.0",
    "@lucia-auth/adapter-prisma": "^4.0.1",
    "@node-rs/argon2": "^1.8.3",
    "@prisma/client": "^6.13.0",
    "@tiptap/core": "^2.26.1",
    "@tiptap/extension-bullet-list": "^2.26.1",
    "@tiptap/extension-list-item": "^2.26.1",
    "@tiptap/pm": "^2.26.1",
    "@tiptap/starter-kit": "^2.26.1",
    "chart.js": "^4.5.0",
    "firebase-scrypt": "^2.2.0",
    "mailtrap": "^3.4.0",
    "oslo": "^1.2.1",
    "sveltekit-superforms": "^2.27.1",
    "validator": "^13.15.15",
    "zod": "^3.25.76"
  },
  "pnpm": {
    "overrides": {
      "devalue": ">=5.3.2",
      "axios": ">=1.12.0"
    }
  }
}
